{
    "version": 2,
    "builds": [
      {
        "src": "src/**/*",
        "use": "@vercel/node"
      },
      {
        "src": "angular.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist/bookstore"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/bookstore/$1"
      }
    ]
  }
  